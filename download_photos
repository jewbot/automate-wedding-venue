from google_images_search import GoogleImagesSearch
import os

# your api key
gis = GoogleImagesSearch('lmao-good-try-get-your-own-api-key', "573677e84b14442e4")

def altElement(a):
    return a[::2]

# define search params:
_search_params = {
    'q': '...',
    'num': 5,
    'safe': 'off',
    'fileType': 'jpg|gif|png'
}

def download_images(query, path):
    
#    for query in queries:
        _search_params['q'] = query
        gis.search(search_params=_search_params)
        for image in gis.results():
            image.download(path)
        print("downloading images for "+ str(query) + " into " + str(path))

